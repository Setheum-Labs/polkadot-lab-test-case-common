{
  "name": "@w3f/polkadot-lab-test-case-common",
  "version": "0.1.0",
  "description": "Common library for all Polkadot Lab test cases",
  "repository": "git@github.com:w3f/helm-ts.git",
  "author": "W3F Infrastructure Team <devops@web3.foundation>",
  "license": "Apache-2.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "lint": "yarn eslint . --ext .js,.jsx,.ts,.tsx",
    "build": "tsc --build tsconfig.json",
    "prepare": "yarn build",
    "pretest": "yarn lint",
    "test": "mocha --timeout 600000 --require ts-node/register --exit test/*.ts test/**/*.ts",
    "start": "node ./dist/index.js start"
  },
  "dependencies": {
    "@w3f/logger": "^0.3.0",
    "@w3f/polkadot-lab-types": "^0.7.0",
    "ws": "^7.3.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.9",
    "@types/chai-as-promised": "^7.1.2",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.11.1",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^6.6.0",
    "js-yaml": "^3.14.0",
    "mocha": "^7.0.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.2"
  }
}
